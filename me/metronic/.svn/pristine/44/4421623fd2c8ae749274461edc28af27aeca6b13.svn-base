    (function() {
        //仿collapse
        $(document).on('click.chevron', '[data-toggle="collapse-chevron"]', function() {
            var $this = $(this);
            var $target = $($this.data("target"));
            if ($target.is(":hidden")) {
                $target.slideDown();
                $this.find(".fa-chevron-down,.fa-chevron-up").removeClass("fa-chevron-down").addClass("fa-chevron-up");
            } else {
                $target.slideUp();
                $this.find(".fa-chevron-down,.fa-chevron-up").removeClass("fa-chevron-up").addClass("fa-chevron-down");
            }
        });
    })();
    </script>
