/* 翻页 */

.pages {
    .clearfix;
    width: 100%;
}

.paginator {
    float: left;
    _display: inline;
    position: relative;
    left: 50%;
    padding: 30px 0 0 0;
    _zoom: 1;
}

.paginator span {
    position: relative;
    left: -50%;
    display: block;
    float: left;
    _display: inline;
    width: auto;
    white-space: nowrap;
    text-align: center;
    text-decoration: none;
    height: 21px;
    margin: 0 !important;
}

.paginator a.paginator,
.paginator a,
.paginator span.cpb {
    position: relative;
    left: -50%;
    display: block;
    float: left;
    _display: inline;
    margin: 0 5px 0 0px!important;
    height: 17px;
    width: auto;
    padding: 3px 10px;
    white-space: nowrap;
    text-align: center;
    border: 1px solid #ccc;
    font-size: 12px;
    line-height: 18px;
    text-decoration: none;
    clear: none;
    _zoom: 1;
    overflow: hidden;
    color: #333333;
}

.paginator span.cpb {
    border: none;
    margin: 0 5px;
    font-weight: bold;
}

.paginator a {
    border: 1px solid #f1f1f1;
    color: #ccc;
    background: #f9f9f9;
    box-sizing: content-box;
    -webkit-box-sizing: content-box;
}

.paginator a:hover {
    border: 1px solid #3d5c99;
    background: #3d5c99;
    color: #fff;
    text-decoration: none;
}

.paginator span a.paginator {
    position: static;
}

.paginator a.paginator {
    background: #f9f9f9;
    border: 1px solid #e6e6e6;
}

.paginator a.paginator:hover {
    border: 1px solid #3d5c99;
    background: #3d5c99;
    color: #fff;
    text-decoration: none;
}

