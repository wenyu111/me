//v1-通话历史
.defined-timeline {
    margin-top: 20px;
    .dt-title {
        text-align: center;
        font-size: 14px;
        span {
            margin: 0 8px;
        }
    }
    .dt-body {
        position: relative;
        //height: 500px;
        .dt-line {
            position: absolute;
            height: 40px;
            background-color: #dadada;
            width: 100%;
            top: 40px;
        }
        .dt-block {
            width: 100%;
            height: 90px;
            //margin-bottom: 10px;
            ul {
                // position: absolute;
                // top: 32px;
                list-style: none;
                width: 100%;
                margin: 0px;
                padding: 0px;
                margin-top: 32px;
                position: relative;
                li {
                    float: left;
                    width: 6.66%;
                    height: 55px;
                    padding-right: 1px;
                    position: relative;
                    &:last-child {
                        width: 6.76%;
                        padding-right: 0px;
                    }
                    span {
                        display: inline-block;
                        float: left;
                        height: 100%;
                        background-color: rgba(178, 229, 254, 0.6);
                        &.green {
                            background-color: rgba(128, 229, 128, 0.6);
                        }
                    }
                    i {
                        position: absolute;
                        display: block;
                        width: 20px;
                        height: 10px;
                        top: 0px;
                        left: 3%;
                        text-align: center;
                        border-radius: 5px;
                        border: 1px solid #000;
                    }
                }
            }
            &.time {
                height: 30px;
                ul {
                    top: 10px;
                    li {
                        em {
                            font-style: normal;
                            font-size: 12px;
                            text-align: left;
                            display: block;
                            padding: 0;
                            margin: 0;
                        }
                        &:last-child {
                            em {
                                float: left;
                                text-align: left;
                                &.last {
                                    float: right;
                                    text-align: right;
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}

