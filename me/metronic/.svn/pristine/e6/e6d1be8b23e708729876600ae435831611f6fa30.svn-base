/* 品牌和城市的选择 */

.advance-select {
    position: relative;
    .form-control {
        line-height: 1.5;
        white-space: nowrap;
        overflow: hidden;
        position: relative;
        &:after {
            content: "\f0dd";
            width: 18px;
            height: 34px;
            position: absolute;
            right: 0;
            padding-left: 5px;
            font: normal normal normal 14px/1 FontAwesome;
            background: #fff;
        }
    }
    .advance-select-cot {
        width: 218px;
        height: 292px;
        overflow: hidden;
        border: 1px solid #e5e5e5;
        position: absolute;
        z-index: 10052;
        top: 35px;
        left: 0;
        background: #FFFFFF;
        display: none;
        .advance-select-reset {
            display: block;
            border-bottom: 1px solid #e5e5e5;
            line-height: 22px;
            font-size: 14px;
            text-align: center;
            color: #000;
            background: #FDFDFD;
            &:hover {
                text-decoration: none;
            }
        }
        address {
            width: 42px;
            height: 270px;
            overflow: hidden;
            float: left;
            padding-bottom: 25px;
            border-right: 1px solid #e5e5e5;
            p {
                transform: translateY(0px);
                transition: all 1s;
                &.top {
                    transform: translateY(-244px);
                }
            }
            span {
                display: block;
                text-align: center;
                line-height: 22px;
                font-size: 14px;
                font-family: "arial";
                font-weight: bold;
                color: #999;
                cursor: pointer;
                &:hover {
                    background: #395da9;
                    color: #fff;
                }
            }
            em {
                display: block;
                height: 24px;
                position: absolute;
                bottom: 0;
                left: 0;
                width: 41px;
                cursor: pointer;
                font-size: 12px;
                border-top: 1px solid #dddddd;
                background: url("../img/arrow.png?20150521") no-repeat 18px 8px #ffffff;
                &.top {
                    background-position: -32px -42px;
                }
            }
        }
        ul {
            float: left;
            margin: 0;
            padding: 0 5px;
            width: 174px;
            height: 270px;
            overflow: hidden;
            overflow-y: scroll;
            li {
                padding: 6px 10px 6px 20px;
                list-style: none;
                border-bottom: 1px solid #e5e5e5;
                font-size: 14px;
                cursor: pointer;
                em {
                    font-family: Arial;
                    font-style: normal;
                    margin-right: 10px;
                }
                &:hover {
                    background: #395da9;
                    color: #FFFFFF;
                }
            }
        }
    }
}

